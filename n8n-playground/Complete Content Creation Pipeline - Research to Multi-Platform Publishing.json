    {
      "id": "prepare-content-data",
      "name": "Prepare Publishing Data",
      "type": "n8n-nodes-base.code",
      "position": [1100, 400],
      "parameters": {
        "jsCode": "// Prepare all content for publishing\nconst outline = $('Generate Content Outline').item.json.message.content;\nconst fullContent = $('Generate Full Article').item.json.message.content;\nconst seoData = $('SEO Quality Check').item.json.message.content;\nconst settings = $('Initialize Settings').item.json;\nconst keywords = $('Keyword Research').item.json.message.content;\n\n// Prepare complete article with FAQ\nlet completeArticle = fullContent;\n\n// Add FAQ section if exists\nif (outline.faqQuestions && outline.faqQuestions.length > 0) {\n  completeArticle += '\\n\\n<h2>Frequently Asked Questions</h2>\\n';\n  outline.faqQuestions.forEach(faq => {\n    completeArticle += `<h3>${faq.question}</h3>\\n<p>${faq.answerOutline}</p>\\n`;\n  });\n}\n\n// Prepare for different platforms\nreturn [{\n  json: {\n    // Common fields\n    title: outline.headline,\n    content: completeArticle,\n    excerpt: outline.metaDescription,\n    slug: outline.urlSlug,\n    \n    // SEO data\n    metaDescription: outline.metaDescription,\n    primaryKeyword: keywords.primaryKeyword,\n    keywords: keywords.secondaryKeywords.join(', '),\n    seoScore: seoData.seoScore,\n    \n    // Images\n    featuredImage: $('Generate Featured Image').item.binary.data,\n    socialImage: $('Generate Social Media Images').item.binary.data,\n    \n    // Publishing info\n    platforms: settings.platforms,\n    topic: settings.topic,\n    audience: settings.audience,\n    tone: settings.tone,\n    \n    // Twitter thread (first 280 chars + link)\n    twitterThread: [\n      `${outline.headline}\\n\\nðŸ§µ Thread (1/${Math.ceil(completeArticle.length / 250)})`,\n      outline.introduction.substring(0, 250) + '...',\n      `Read the full article: [LINK]`\n    ],\n    \n    // LinkedIn post\n    linkedinPost: `${outline.headline}\\n\\n${outline.introduction}\\n\\n${outline.sections[0].keyPoints.slice(0, 3).map(p => 'â€¢ ' + p).join('\\n')}\\n\\nRead more: [LINK]`,\n    \n    // Analytics\n    timestamp: settings.timestamp,\n    publishedUrls: {}\n  }\n}];"
      },
      "typeVersion": 2
    },
